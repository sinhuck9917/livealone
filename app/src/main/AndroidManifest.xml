<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.livealone4">

    <application
        android:allowBackup="true"
        android:icon="@drawable/livealone1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/livealone1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activities.SigninActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.LiveAloneActivity" />
        <activity android:name=".Activities.CandidateListActivity" />
        <activity android:name=".Activities.RatingActivity" />

        <service android:name=".Services.LiveAloneService" />

        <activity android:name=".Activities.MessageActivity" />
        <activity android:name=".Activities.DataGeneratorActivity" />
        <activity android:name=".Activities.UserProfileActivity" />
        <activity android:name=".Activities.SignUpActivity"></activity>
    </application>

</manifest>